{
  "name": "alpari/messenger-bundle",
  "type": "symfony-bundle",
  "description": "Integrates symfony/messenger component into symfony 2.7+",
  "keywords": [],
  "license": "MIT",
  "authors": [
    {
      "name": "Samuel Roze",
      "email": "samuel.roze@gmail.com"
    },
    {
      "name": "Symfony Community",
      "homepage": "https://symfony.com/contributors"
    },
    {
      "name": "Efimov Evgenij",
      "email": "edefimov.it@gmail.com"
    }
  ],
  "require": {
    "php": "~7.1",
    "psr/container": "^1.0",
    "symfony/config": "~2.7 || ~3.0 || ~4.0.0",
    "symfony/console": "~2.7 || ~3.0 || ~4.0.0",
    "symfony/dependency-injection": "~2.7 || ~3.0 || ~4.0.0",
    "symfony/http-kernel": "~2.7 || ~3.0 || ~4.0.0",
    "symfony/messenger": "^4.1.1"
  },
  "conflict": {
    "symfony/framework-bundle": ">=4.1"
  },
  "require-dev": {
    "doctrine/orm": "~2.4",
    "phpunit/phpunit": "~7",
    "symfony/framework-bundle": "~2.7 || ~3.0 || ~4.0",
    "symfony/serializer": "~2.7 || ~3.0 || ~4.0",
    "symfony/validator": "~2.7 || ~3.0 || ~4.0",
    "symfony/yaml": "~2.7 || ~3.0 || ~4.0"
  },
  "autoload": {
    "psr-4": {
      "Symfony\\Bundle\\MessengerBundle\\": "src/Bundle",
      "Symfony\\Bridge\\": "src/Bridge",
      "Symfony\\Backport\\": "src/Backport"
    },
    "files": [
      "src/Backport/autoload.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Symfony\\Bundle\\MessengerBundle\\": "tests/Bundle"
    }
  }
}
